<window width="400px" closable="false" position="center" title="Select Primary Key">
    <grid id="grid">
        <columns sizable="true">
            <column label="Activity" sort="auto"/>
            <column label="PrimaryKey"/>
        </columns>
        <rows id="rows"/>
	</grid>
    <div align="center">
        <button id="cancelButton" label="Cancel" image="img/cancel2.png"></button>
        <button id="okButton" label="Ok" image="img/ok.png"></button>
    </div>
</window>