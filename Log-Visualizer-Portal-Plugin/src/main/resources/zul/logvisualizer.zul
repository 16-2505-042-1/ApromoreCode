<window width="600px" closable="false" position="center" title="Log Visualizer"
    apply="org.apromore.plugin.portal.logvisualizer.LogVisualizerController">
    <vbox>
        <grid>
            <columns sizable="true">
                <column width="40%"/>
                <column width="60%"/>
            </columns>
            <rows>
                <row>
                    <label>Percentage Activities</label>
                    <slider id="slider1" mold="scale" maxpos="100" curpos="100"/>
                </row>
                <row>
                    <label>Percentage Arcs</label>
                    <slider id="slider2" mold="scale" maxpos="100" curpos="100"/>
                </row>
            </rows>
        </grid>
        <div align="center">
            <button id="okButton" label="OK" image="img/ok.png" disabled="false"/>
            <button id="cancelButton" label="Cancel" image="img/cancel2.png"/>
        </div>
    </vbox>
</window>
